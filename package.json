{
  "name": "rn_fashion_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:staging": "ENVFILE=.env.staging react-native run-android --variant=stagingDebug --appId com.rn_fashion_app.staging",
    "ios:staging": "react-native run-ios --scheme=\"RN Fashion Staging\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean:android": "cd android && ENVFILE=.env.staging ./gradlew clean && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "apk": "cd android && ENVFILE=.env ./gradlew app:assembleRelease && cd app && cd build && cd outputs && cd apk && cd production && cd release && open .",
    "bundle": "cd android && ENVFILE=.env ./gradlew bundleRelease && cd app && cd build && cd outputs && cd production && cd release && open ."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.4",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/drawer": "^5.9.0",
    "@react-navigation/native": "^5.7.1",
    "@react-navigation/stack": "^5.7.1",
    "@shopify/restyle": "^1.1.0",
    "formik": "^2.1.5",
    "moment": "^2.29.1",
    "react": "16.13.1",
    "react-native": "0.63.0",
    "react-native-config": "^1.4.2",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.2",
    "react-native-reanimated": "^1.9.0",
    "react-native-redash": "^14.2.3",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-screens": "^2.9.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.0",
    "react-native-vector-icons": "^8.1.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.1",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/yup": "^0.29.4",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.0.4",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
